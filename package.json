{
  "name": "mykey",
  "displayName": "mykey",
  "description": "better move, better code",
  "version": "0.1.0",
  "publisher":"joseph",
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*",
    "onCommand:type",
    "onCommand:mykey.toggle"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mykey.toggle",
        "title": "Toggle MyKey"
      }
    ],
    "keybindings": [
      {
        "command": "mykey.moveLeft",
        "key": "j",
        "mac": "j",
        "when": "editorFocus && mykey.mode != 'Insert'"
      },
      {
        "command": "mykey.moveRight",
        "key": "l",
        "mac": "l",
        "when": "editorFocus && mykey.mode != 'Insert'"
      },
      {
        "command": "mykey.moveUp",
        "key": "i",
        "mac": "i",
        "when": "editorFocus && mykey.mode != 'Insert'"
      },
      {
        "command": "mykey.moveDown",
        "key": "k",
        "mac": "k",
        "when": "editorFocus && mykey.mode != 'Insert'"
      },
      {
        "command": "mykey.moveLeftWithSelection",
        "key": "shift+j",
        "mac": "shift+j",
        "when": "editorFocus && mykey.mode != 'Insert'"
      },
      {
        "command": "mykey.moveRightWithSelection",
        "key": "shift+l",
        "mac": "shift+l",
        "when": "editorFocus && mykey.mode != 'Insert'"
      },
      {
        "command": "mykey.moveUpWithSelection",
        "key": "shift+i",
        "mac": "shift+i",
        "when": "editorFocus && mykey.mode != 'Insert'"
      },
      {
        "command": "mykey.moveDownWithSelection",
        "key": "shift+k",
        "mac": "shift+k",
        "when": "editorFocus && mykey.mode != 'Insert'"
      },
      {
        "command": "mykey.copy",
        "key": "c",
        "mac": "c",
        "when": "editorFocus && mykey.mode != 'Insert'"
      },
      {
        "command": "mykey.paste",
        "key": "v",
        "mac": "v",
        "when": "editorFocus && mykey.mode != 'Insert'"
      },
      {
        "command": "mykey.cut",
        "key": "x",
        "mac": "x",
        "when": "editorFocus && mykey.mode != 'Insert'"
      },
      {
        "command": "mykey.toggle",
        "key": "Escape",
        "mac": "Escape",
        "when": "editorTextFocus && mykey.mode == 'Insert'"
      },
      {
        "command": "mykey.toggle",
        "key": "space",
        "mac": "space",
        "when": "editorTextFocus && mykey.mode != 'Insert'"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack --config ./build/node-extension.webpack.config.js",
    "watch": "webpack --watch --config ./build/node-extension.webpack.config.js",
    "package": "webpack --mode production --devtool hidden-source-map --config ./build/node-extension.webpack.config.js",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.52.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "eslint": "^7.15.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.1.2",
    "vscode-test": "^1.4.1",
    "ts-loader": "^8.0.11",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0"
  }
}
